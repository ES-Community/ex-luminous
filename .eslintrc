{
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "script",
    "ecmaVersion": 2019
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "strict": ["error", "global"]
  },
  "globals": {
    "game": true,
    "playerName": true,
    "server": true,
    "grpcClient": true,
    "isHost": true,
    "CraftStudio": true
  }
}
